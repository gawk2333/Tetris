    "react-toastify": "^9.1.1",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.4",
    "sqlite3": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.13.1",

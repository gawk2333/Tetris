  "bugs": {
    "url": "https://github.com/gawk2333/Tetris/issues"
  },
  "homepage": "https://github.com/gawk2333/Tetris#readme",
  "eslintConfig": {
    "extends": "eda",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
